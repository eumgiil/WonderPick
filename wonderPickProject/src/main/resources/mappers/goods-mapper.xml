<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="goodsMapper">
  
  <select id="selectListCount" resultType="_int">
	 select
            *
     from
            board
	  where
	        status = 'Y'
	  and
	       board_type = 'G'
  </select>
  
  
  
  
  
  
  
  
  
  
  
  
  </mapper>